{
    "name": "jgrossi/corcel",
    "replace": { "juniorgrossi/corcel": "self.version" },
    "description": "Use WordPress backend with Laravel or any PHP framework",
    "homepage": "http://github.com/corcel/corcel",
    "license": "MIT",
    "authors": [
        {
            "name": "Junior Grossi",
            "email": "juniorgro@gmail.com",
            "homepage": "http://jgrossi.com"
        }
    ],
    "require": {
        "php": "^8.0.2 |^8.2",
        "illuminate/database": "^v10.13.0",
        "illuminate/filesystem": "^v10.13.0",
        "illuminate/support": "^v10.13.0",
        "fakerphp/faker": "^v1.22.0",
        "bordoni/phpass": "0.3.4",
        "thunderer/shortcode": "^v0.7.5"
    },
    "require-dev": {
        "laravel/legacy-factories": "^v1.3.2",
        "orchestra/testbench": "^v8.5.5",
        "symfony/thanks": "^v1.2.10",
        "mockery/mockery": "^1.5.1",
        "dms/phpunit-arraysubset-asserts": "^v0.4.0"
    },
    "autoload": {
        "psr-4": {
            "Corcel\\": "src/",
            "Corcel\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Corcel\\Laravel\\CorcelServiceProvider"
            ]
        }
    },
    "config": {
        "allow-plugins": {
            "symfony/thanks": false
        }
    }
}
